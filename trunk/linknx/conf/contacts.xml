<?xml version="1.0" ?>
<config>
    <objects>
        <object type="contact" id="FirstLast" firstName="First" lastName="Last" homePhone="+3231111111" officePhone="+3231111111" mobilePhone="+32111111111" email="me@mail.com" birthDay="18/2">First Last</object> 
    </objects>
    <rules>
        <rule id="__bday_FirstLast">
            <condition type="timer" trigger="true">
                <at month="2" day="18" hour="6" min="30" />
            </condition>
            <actionlist>
                <action type="send-sms" id="FirstLast" value="Happy Birthday!" />
            </actionlist>
        </rule>
    </rules>
    <services>
        <xmlserver type="inet" port="10280"/>
    </services>
</config>
